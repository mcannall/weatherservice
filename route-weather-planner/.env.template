# Google Maps API Key - Required for geocoding and directions
GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# API Service URL - Points to the weather API service
# In Kubernetes, this will be set to http://api:80
# For local development, use http://localhost:30080
API_URL=http://localhost:30080

# Flask Configuration
FLASK_APP=app.py
FLASK_ENV=development